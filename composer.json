{
    "authors": [
        {
            "email": "hswong3i@pantarei-design.com",
            "homepage": "http://hswong3i.net",
            "name": "Wong Hoi Sing Edison"
        }
    ],
    "autoload": {
        "psr-4": {
            "AuthBucket\\Push\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AuthBucket\\Push\\Tests\\": "tests/"
        }
    },
    "description": "Push notifications for mobile devices based on the Symfony Components",
    "extra": {
        "branch-alias": {
            "dev-develop": "1.x-dev",
            "dev-master": "1.0.x-dev"
        }
    },
    "homepage": "https://github.com/authbucket/push-php",
    "keywords": [
        "apns",
        "gcm",
        "notification",
        "oauth2",
        "psr-1",
        "psr-2",
        "psr-3",
        "psr-4",
        "push",
        "silex"
    ],
    "license": "MIT",
    "name": "authbucket/push-php",
    "require": {
        "authbucket/oauth2-php": "~3.0",
        "guzzlehttp/guzzle": "~6.0",
        "php": ">=5.5.9",
        "psr/log": "~1.0",
        "symfony/http-foundation": "~2.8",
        "symfony/http-kernel": "~2.8",
        "symfony/security": "~2.8",
        "symfony/validator": "~2.8"
    },
    "require-dev": {
        "doctrine/data-fixtures": "~1.0",
        "doctrine/orm": "~2.2",
        "ext-pdo_sqlite": "*",
        "monolog/monolog": "~1.4",
        "silex/silex": "~1.3",
        "symfony/browser-kit": "~2.8",
        "symfony/config": "~2.8",
        "symfony/css-selector": "~2.8",
        "symfony/form": "~2.8",
        "symfony/phpunit-bridge": "~2.8",
        "symfony/process": "~2.8",
        "symfony/translation": "~2.8",
        "symfony/twig-bridge": "~2.8",
        "twig/twig": "~1.8"
    },
    "scripts": {
        "coveralls": "coveralls -v; exit 0",
        "php-cs-fixer": "php-cs-fixer fix --config-file .php-cs-fixer.php; exit 0",
        "phpunit": "phpunit -c phpunit.xml.dist; exit 0",
        "sami": "sami.php update .sami.php; exit 0"
    },
    "type": "library"
}
