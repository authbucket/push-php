    "require": {
        "authbucket/oauth2-php": "~2.3",
        "guzzle/guzzle": "~3.9",
        "php": ">=5.3.9",
        "symfony/http-foundation": ">=2.3,<2.6-dev",
        "symfony/http-kernel": ">=2.3,<2.6-dev",
        "symfony/security": ">=2.3,<2.6-dev",
        "symfony/serializer": ">=2.3,<2.6-dev",
