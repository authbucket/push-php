        "symfony/form": ">=2.3,<2.6-dev",
        "symfony/locale": ">=2.3,<2.6-dev",
        "symfony/process": ">=2.3,<2.6-dev",
        "symfony/translation": ">=2.3,<2.6-dev",
        "symfony/twig-bridge": ">=2.3,<2.6-dev",
